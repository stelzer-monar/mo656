</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#analgésico">
  <rdf:type rdf:resource="#Action"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#dor">
  <rdf:type rdf:resource="#Use"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#dipirona">
  <rdf:type rdf:resource="#Ingredient"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#dor de cabeça">
