</owl:Class>

<owl:Class rdf:about="#Use">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#doril">
  <rdf:type rdf:resource="#Drug"/>
  <isComposedOf rdf:resource="#Dipirona"/>
  <actAs rdf:resource="#analgésico"/>
  <isUsedFor rdf:resource="#dor"/>
